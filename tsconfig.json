{
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "baseUrl": ".",
    "importHelpers": true,
    "module": "commonjs",
    "target": "es2017",
    "strict": false,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      "@conf/*": [
        "config/*"
      ],
      "~/*": [
        "./*"
      ]
    },
  },
  "allowSyntheticDefaultImports": true,
  "skipLibCheck": true,
  "noEmit": true,
  "files": [],
  "references": [{ "path": "./tsconfig.node.json" }, { "path": "./tsconfig.web.json" }]
}
